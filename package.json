    "ts-jest": "^26.5.5",
    "vue-jest": "^3.0.7",
    "vuetify": "^2.4.7"
  },
  "resolutions": {
    "eslint-plugin-vuetify/eslint-plugin-vue": "^7.1.0"
  },
  "engines": {
    "node": "^14.5.0"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      },
      "vue-jest": {
        "tsConfig": true
      }
    },
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
