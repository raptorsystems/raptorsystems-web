    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@mdi/js": "^5.9.55",
    "@nuxt/types": "^2.15.3",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
